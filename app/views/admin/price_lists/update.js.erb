<% if @price_list.errors.blank? %>
  $("#edit-price-list").modal("hide");
  $("#price-lists").html("<%= j render partial: :price_list, collection: @price_lists, as: :price_list %>");
<% else %>
  $("#edit-price-list .bootstrap-dialog-message").html("<%= j render 'form', price_list: @price_list, \
    form_path: admin_price_list_path(@price_list) %>".replace(/\n/g, ''));
<% end %>
$("#flash-messages").html("<%= j render 'layouts/shared/flash' %>");
