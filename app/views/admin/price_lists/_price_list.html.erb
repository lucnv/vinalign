<tr>
  <td><%= price_list.id %></td>
  <td><%= price_list.item %></td>
  <td><%= price_list.price %></td>
  <td>
    <%= link_to edit_admin_price_list_path(price_list), remote: true,
      class: "btn btn-warning btn-xs btn-flat" do %>
      <i class="fa fa-pencil-square-o"></i>
      <%= t ".edit" %>
    <% end %>
    <%= link_to admin_price_list_path(price_list), method: :delete,
      data: {confirm: t(".confirm_delete")}, class: "btn btn-danger btn-xs btn-flat" do %>
      <i class="fa fa-pencil-square-o"></i>
      <%= t ".delete" %>
    <% end %>
  </td>
</tr>
