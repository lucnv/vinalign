BootstrapDialog.show({
  title: "<%= t '.edit_price_list' %>",
  id: "edit-price-list",
  message: "<%= j render 'form', price_list: @price_list, \
    form_path: admin_price_list_path(@price_list) %>".replace(/\n/g, ''),
  closeByBackdrop: false,
  closeByKeyboard: false,
  buttons: [{
    label: "<%= t '.cancel' %>",
    action: function(dialog){
      dialog.close();
    }
  },
  {
    label: "<%= t '.update' %>",
    cssClass: "btn-primary",
    id: "button-update-prict-list",
    action: function(dialogRef){
      dialogRef.getModalBody().find("form").submit();
    }
  }]
});
