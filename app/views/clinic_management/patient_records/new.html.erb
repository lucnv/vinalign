<% content_for :patient_records_activation, "active" %>
<section class="content-header">
  <h1><%= t ".new_patient_record" %></h1>
</section>
<section class="content">
  <div class="box">
    <div class="box-body">
      <%= form_for [:clinic_management, @patient_record], html: {class: "form-horizontal", multipart: true} do |f| %>
        <%= render "form", f: f, patient_record: @patient_record %>
      <% end %>
    </div>
  </div>
</section>
